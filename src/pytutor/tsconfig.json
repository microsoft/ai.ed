{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"lib": [
			"es6"
		],
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
